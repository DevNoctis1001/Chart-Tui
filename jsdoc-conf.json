{
    "source": {
        "include": ["src/js", "README.md"],
        "exclude": [
            "src/js/code-snippet-util.js"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "template": {
        "name": "Chart"
    },
    "opts": {
        "recurse": true,
        "destination": "doc",
        "tutorials": "examples",
        "template": "node_modules/tui-doc-template",
        "package": "package.json"
    }
}
